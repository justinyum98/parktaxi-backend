{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
      "eol-last": "error",
      "sort-imports": "error",
      "comma-dangle": "error",
      "prefer-arrow-callback": "error",
      "no-console": "warn"
    }
}